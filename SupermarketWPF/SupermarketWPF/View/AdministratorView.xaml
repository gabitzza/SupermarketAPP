<Window x:Class="SupermarketWPF.View.AdministratorView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AdministratorView" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Content="Producători" Command="{Binding OpenProducatoriViewCommand}" Width="200" Margin="5"/>
            <Button Content="Utilizatori" Command="{Binding OpenUtilizatoriViewCommand}" Width="200" Margin="5"/>
            <Button Content="Categorii" Command="{Binding OpenCategoriiViewCommand}" Width="200" Margin="5"/>
            <Button Content="Produse" Command="{Binding OpenProduseViewCommand}" Width="200" Margin="5"/>
            <Button Content="Stocuri" Command="{Binding OpenStocuriViewCommand}" Width="200" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>