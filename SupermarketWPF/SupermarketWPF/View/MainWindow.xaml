<Window x:Class="SupermarketWPF.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SupermarketWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button Content="Administrator" Command="{Binding OpenAdministratorViewCommand}" Width="200" Margin="10"/>
            <Button Content="Casier" Command="{Binding OpenCasierViewCommand}" Width="200" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
